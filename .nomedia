// logo 默认隐藏
$('#' + this._opts.topId + '_logo').css({
    'height': '24px',
    'display': 'none'
})

$('#' + this._opts.topId + '_back').css({
    'width': '40px',
    'height': '40px',
    'padding': '2px',
    'border-radius': '50%',
    'display': 'flex',
    'justify-content': 'center',
    'align-items': 'center',
    'box-sizing': 'border-box'
})

if (this._opts.openBordLine == true) {
    $('#' + this._opts.topId + '_back').css({
        'border': '1px solid ' + this._opts.colorBtnTouch
    })
} else {
    $('#' + this._opts.topId + '_back').css({
        'border': ''
    })
}

